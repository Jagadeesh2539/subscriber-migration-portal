{
  "name": "subscriber-portal-enhanced",
  "version": "3.0.0",
  "private": true,
  "engines": {
    "node": ">=18.20.0 <25.0.0",
    "npm": ">=10.0.0"
  },
  "resolutions": {
    "webpack": "4.44.2",
    "terser-webpack-plugin": "4.2.3",
    "schema-utils": "2.7.1",
    "ajv": "6.12.6",
    "ajv-keywords": "3.5.2",
    "css-loader": "5.2.7",
    "postcss-loader": "4.3.0",
    "sass-loader": "10.4.1",
    "fork-ts-checker-webpack-plugin": "4.1.6",
    "mini-css-extract-plugin": "0.11.3",
    "semver": "7.5.4",
    "nth-check": "^2.1.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.4",
    "axios": "^0.27.2",
    "@mui/material": "^5.15.11",
    "@mui/icons-material": "^5.15.11",
    "@mui/lab": "^5.0.0-alpha.153",
    "@mui/x-data-grid": "^5.17.20",
    "@mui/x-date-pickers": "^5.0.20",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "react-scripts": "4.0.3",
    "recharts": "^2.12.7",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "formik": "^2.4.5",
    "yup": "^1.3.3",
    "react-error-boundary": "^3.1.4",
    "react-hot-toast": "^2.4.1",
    "dayjs": "^1.11.10",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.8",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.2.5",
    "typescript": "^4.9.5",
    "@types/react": "^17.0.85",
    "@types/react-dom": "^17.0.20",
    "@types/node": "^16.18.96",
    "@types/lodash": "^4.17.7",
    "@types/react-window": "^1.8.8",
    "cross-env": "^7.0.3",
    "npm-force-resolutions": "^0.0.10"
  },
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true NODE_OPTIONS=--openssl-legacy-provider react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules package-lock.json build",
    "reinstall": "npm run clean && npm install --legacy-peer-deps",
    "preinstall": "npx npm-force-resolutions",
    "build:docker": "docker build --target artifact --output ./build/ -f Dockerfile ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "warn",
      "no-unused-vars": "warn",
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}