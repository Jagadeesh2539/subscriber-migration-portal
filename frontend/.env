# Subscriber Migration Portal - Frontend Environment
# Single Source of Truth Configuration

# API Configuration - Standardized Variable Name
REACT_APP_API_BASE_URL=https://hsebznxeu6.execute-api.us-east-1.amazonaws.com/prod

# Application Configuration
REACT_APP_VERSION=2.0.0-production
REACT_APP_ENVIRONMENT=production

# Feature Flags - Professional Portal
REACT_APP_LEGACY_ENABLED=true
REACT_APP_CLOUD_ENABLED=true
REACT_APP_DUAL_PROV_ENABLED=true
REACT_APP_PROVISIONING_MODES=legacy,cloud,dual_prov

# Migration Features
REACT_APP_BULK_OPERATIONS_ENABLED=true
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_MONITORING_ENABLED=true

# Security
REACT_APP_JWT_ENABLED=true
REACT_APP_AUDIT_LOGGING=true

# Development Settings
GENERATE_SOURCEMAP=false
REACT_APP_DEBUG=false

# Build Optimization
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=0
